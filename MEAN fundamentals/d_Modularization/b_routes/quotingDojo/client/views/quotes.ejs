<body>
  <script>
    $(document).ready(function(){
      
    });
  </script>
<div class="body">
  <div class="container">
    <h1>Quotes!</h1>
    <div class="quotes">
      <% for (var i = 0; i < quotes.length; i++) { %>
        <div style="color: red">
          "<%= quotes[i].quote %>"
        </div>
        <div style="color: blue">
          -<%= quotes[i].name %> at <%= quotes[i].createdAt %>
        </div>
      <% } %>
    </div>
</div>
</body>
</html>